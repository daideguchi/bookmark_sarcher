{
  "manifest_version": 3,
  "name": "Bookmark Search",
  "version": "1.0",
  "description": "Search bookmarks using input text.",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Bookmark Search"
  },
  "permissions": ["bookmarks"],
  "icons": {
    "48": "icon.png"
  }
}
